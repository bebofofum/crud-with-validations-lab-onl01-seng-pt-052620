<h2>Add a Song</h2>

<%= form_for @song do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title %><br>
  <%= f.label :genre %>:
  <%= f.text_field :genre %><br>
  <%= f.label :artist_name, "Artist Name" %>:
  <%= f.text_field :artist_name %><br>
  <%= f.label :released %>:
  <%= f.check_box :released %><br>
  <%= f.label :release_year, "Year Released" %>:
  <%= f.text_field :release_year %><br>
  <%= f.submit %>
    <% end %>