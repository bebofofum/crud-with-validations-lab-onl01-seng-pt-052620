<h2>Edit "<%= @song.title %>"</h2>
<%= form_for @song do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title %><br>
  <%= f.label :genre %>:
  <%= f.text_field :genre %><br>
  <%= f.label :artist_name, "Artist Name" %>:
  <%= f.text_field :artist_name %><br>
  <%= f.label :released, "Song Released?"  %>:
  <%= f.check_box :released %>
  <%= f.label :release_year, "Year Released" %>:
  <%= f.text_field :release_year %><br>
  <%= f.submit %>
    <% end %>
